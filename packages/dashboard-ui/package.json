{
  "name": "@pylon-dev/dashboard-ui",
  "version": "0.1.4",
  "description": "Shared dashboard UI components for Pylon",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tailwind-preset": {
      "types": "./dist/tailwind-preset.d.ts",
      "import": "./dist/tailwind-preset.mjs",
      "require": "./dist/tailwind-preset.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resolve-from": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
